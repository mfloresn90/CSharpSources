@model List<sBlog.Net.Domain.Entities.CategoryEntity>

<div class="panel panel-danger">
	<div class="panel-heading">
	<h3 class="panel-title">categories</h3>
	</div>
	<div class="list-group">
        @foreach(var category in Model)
        {
            var url = Url.RouteUrl("Category", new { categoryName = category.CategorySlug });
            <a class="list-group-item" href="@url" title="@category.CategoryName">@category.CategoryName</a>
        }
	</div>
</div>
